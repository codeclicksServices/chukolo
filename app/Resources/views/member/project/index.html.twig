    <hr />



    <section class="box-typical">
        <header class="box-typical-header">
            <div class="tbl-row">
                <div class="tbl-cell tbl-cell-title">
                    <h3>Browse All Jobs</h3>
                </div>

            </div>
        </header>
        <div class="box-typical-body">
            <div class="table-responsive">
                <table class="table table-hover">

                    {% if  allJobs.getTotalItemCount %}

                        <thead>
                        <tr>

                            <th>Project</th>
                            <th>People Bidding
                                <span class="hint-circle green"
                                      data-toggle="tooltip"
                                      data-placement="top"
                                      title="Number of freelancers bidding for this project">?</span></th>
                            <th>Bidding Ends</th>

                            <th>Price
                                <span class="hint-circle green"
                                      data-toggle="tooltip"
                                      data-placement="top"
                                      title="The employers budget for this project">?</span></th>
                            <th>Project Status</th>
                        </tr>
                        </thead>
                        <tbody>
                        {% for job in allJobs %}

                            <tr>

                                <td>
                                    <a href="{{ path('project_details', { 'projectId': job.id }) }}">
                                    {{ job.name |title }}<br />
                                    </a>
                                    {{ job.description |capitalize }}<br />

                                    <p class="card-footer">
                                        {% for skill in job.skill %}
                                            <a  href="#">{{ skill.name}}</a>
                                        {% endfor %}
                                    </p>

                                </td>


                                <td class="color-blue-grey-lighter">
                                    {{ job.bid |length }}

                                </td>
                                <td>
                                    {{ job.bidEnd |date }}
                                </td>
                                <td>
                                    {{ job.budget.budgetname }}
                                </td>
                                <td>
                                    {{ job.state |capitalize}}
                                </td>
                            </tr>

                        {% endfor %}

                        <tr><td colspan="4">{{ knp_pagination_render(allJobs) }}</td></tr>
                        </tbody>

                    {% else %}
                        <tr><td colspan="4">Oh Crap !!!, No Job here check back again soonest </td></tr>
                    {% endif %}
                </table>
            </div>
        </div><!--.box-typical-body-->


    </section>





</div>
