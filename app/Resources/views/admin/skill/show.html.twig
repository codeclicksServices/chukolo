{% extends 'base.html.twig' %}

{% block body %}
    <h1>Skill</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ skill.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ skill.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ skill.description }}</td>
            </tr>
            <tr>
                <th>Visible</th>
                <td>{{ skill.visible }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('skill_index') }}">Back to the list</a>
        </li>
    </ul>
{% endblock %}
